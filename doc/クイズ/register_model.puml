@startuml

package domain {

  package 登録申請書 {
    class 三択問題登録申請書 {
      - 問題文
      - 選択肢A
      - 選択肢B
      - 選択肢C
      - 正解選択肢
      - 解説
    }
  }

  class "三択クイズ<Entity>" as 三択クイズ {
  }

  class 三択クイズRepository {
    + 三択クイズ番号 番号を払い出す()
    + void 三択クイズを登録する(クイズ)
  }

  class 正解選択肢 {
  }

  class 三択クイズ番号 {
    - Integer
  }

  class 問題文 {
    - String
  }
  class 解説 {
    - String
  }
  class 選択肢 {
    - String
  }

  三択問題登録申請書 --> 三択クイズ
  三択クイズRepository --> 三択クイズ
  三択クイズRepository --> 三択クイズ番号
  三択クイズ --> 三択クイズ番号
  三択クイズ --> 三択問題
  三択クイズ --> 三択回答
  三択問題 --> 問題文
  三択問題 --> 選択肢A
  三択問題 --> 選択肢B
  三択問題 --> 選択肢C
  三択回答 --> 正解選択肢
  三択回答 --> 解説
  選択肢A --> 選択肢
  選択肢B --> 選択肢
  選択肢C --> 選択肢
  正解選択肢 --> 選択肢
@enduml
